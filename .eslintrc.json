{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "semi":"error",
    "no-console":"off",
    "no-useless-escape":"off",
    "no-inner-declarations":"warn"
  }
}
